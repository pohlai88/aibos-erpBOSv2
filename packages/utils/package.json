{
    "name": "@aibos/utils",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "scripts": {
        "build": "tsup",
        "type-check": "tsc -p tsconfig.build.json --noEmit",
        "clean": "rimraf dist node_modules/.cache"
    },
    "dependencies": {
        "zod": "4.1.12"
    },
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "5.9.3",
        "rimraf": "6.0.1"
    }
}