{
  "name": "@aibos/auth",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts --format esm,cjs --dts --sourcemap --clean",
    "type-check": "tsc -p tsconfig.build.json --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "zod": "4.1.12"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "5.9.3",
    "rimraf": "6.0.1"
  }
}