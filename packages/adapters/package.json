{
    "name": "@aibos/adapters",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "sideEffects": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./core-ledger/*": {
            "import": "./dist/core-ledger/*.js",
            "types": "./dist/core-ledger/*.d.ts"
        },
        "./db/*": {
            "import": "./dist/db/*.js",
            "types": "./dist/db/*.d.ts"
        },
        "./shared/*": {
            "import": "./dist/shared/*.js",
            "types": "./dist/shared/*.d.ts"
        }
    },
    "scripts": {
        "build": "tsc -b",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "dev": "tsx watch ./src/index.ts",
        "test": "vitest run"
    },
    "dependencies": {
        "drizzle-orm": "~0.34.1",
        "postgres": "~3.4.7",
        "pino": "^9.13.1",
        "@aibos/ports": "workspace:*"
    },
    "devDependencies": {
        "typescript": "5.9.3",
        "vitest": "^3.2.4",
        "tsx": "4.20.6"
    }
}